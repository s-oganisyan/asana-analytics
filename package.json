{
    "name": "asana-analystics",
    "version": "1.0.0",
    "description": "",
    "main": "src/app.ts",
    "scripts": {
        "start": "tsc && node dist/src/app.js",
        "dev": "nodemon",
        "test": "jest --passWithNoTests && npm run lint",
        "lint": "eslint . --ext .js,.ts,",
        "lint:fix": "npm run lint -- --fix"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm test"
        }
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@google-cloud/bigquery": "^5.5.0",
        "@google-cloud/storage": "^5.7.3",
        "asana": "^0.18.5",
        "body-parser": "^1.19.0",
        "celebrate": "^13.0.4",
        "cors": "^2.8.5",
        "cron": "^1.8.2",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "moment-timezone": "^0.5.32",
        "reflect-metadata": "^0.1.13",
        "typedi": "^0.8.0",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@types/express": "^4.17.8",
        "@types/jest": "^26.0.14",
        "@types/node": "^14.11.2",
        "@typescript-eslint/eslint-plugin": "^4.13.0",
        "@typescript-eslint/parser": "^4.13.0",
        "eslint": "^7.18.0",
        "eslint-config-prettier": "^7.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-pretty-imports": "^1.1.3",
        "eslint-plugin-promise": "^4.2.1",
        "husky": "^3.1.0",
        "jest": "^26.4.2",
        "nodemon": "^2.0.4",
        "prettier": "^2.2.1",
        "ts-jest": "^26.4.0",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.3"
    },
    "jest": {
        "transform": {
            ".(ts)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
        "moduleFileExtensions": [
            "ts",
            "js"
        ],
        "testEnvironment": "node"
    }
}
